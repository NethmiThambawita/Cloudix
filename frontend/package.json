{
  "name": "erp-crm-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
   "build": "node node_modules/vite/bin/vite.js build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "^5.12.2",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.4.21"
  },
  "services": {
    "type": "web",
    "name": "cloudix-frontend",
    "env": "static",
    "buildCommand": "npm install && npm run build",
    "staticPublishPath": "./build",
    "routes": [
      {
        "type": "rewrite",
        "source": "/*",
        "destination": "/index.html"
      }
    ]
  }
}